Xft.dpi       : 96
Xft.antialias : false
Xft.rgba      : rgb
Xft.hinting   : true
Xft.hintstyle : hintslight

URxvt.depth           : 32
URxvt.geometry        : 90x30
URxvt.transparent     : false
URxvt.fading          : 0
URxvt.visualBell      : false
URxvt.loginShell      : true
URxvt.saveLines       : 2048
URxvt.internalBorder  : 3
URxvt.lineSpace       : 0
URxvt.allow_bold      : true
URxvt*font            : xft:Iosevka Term Light:size=15
URxvt.boldFont        : xft:Iosevka Term Medium:style=bold:size=15
URxvt.italicFont      : xft:Iosevka Term Medium:style=italic:size=15
URxvt.boldItalicFont  : xft:Iosevka Term Medium:style=medium:size=15
URxvt*letterSpace     : -1
URxvt.scrollStyle     : rxvt
URxvt.scrollBar       : false
URxvt.perl-ext-common : default,matcher
URxvt.matcher.button  : 1
URxvt.urlLauncher     : firefox
URxvt.cursorBlink     : false
URxvt.cursorColor     : #d54e53
URxvt.cursorUnderline : false
URxvt.pointerBlank    : true

